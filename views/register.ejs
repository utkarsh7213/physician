                    maxlength="100">
                </div>
                <div class="form-field">
                  <label for="practice-address-1">Practice Address Line 1:</label>
                  <input type="text" placeholder="Practice Address Line 1" id="practice-address-1" name="address_1"
                    maxlength="100" required>
                </div>
                <div class="form-field">
                  <label for="practice-address-2">Practice Address Line 2:</label>
                  <input type="text" placeholder="Practice Address Line 2" id="practice-address-2" name="address_2"
                    maxlength="100">
                </div>
                <div class="form-field">
                  <label for="practice-city">Practice City:</label>
                  <input type="text" placeholder="Practice City" id="practice-city" name="city" maxlength="50" required>
                </div>
                <div class="form-field">
                  <label for="practice-state">Practice State:</label>
                  <select id="practice-state" name="practice_state">
                    <option value="AL">AL</option>
