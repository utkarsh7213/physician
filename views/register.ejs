    </div><!-- end /.page-wrap-container -->
  </div><!-- end /.page-wrap -->
  <script src="https://www.skinmedicinals.com/portal/dist/js/production.min.js"></script>
  <script>
    jQuery('#password').on('focus', function (e) {
      jQuery('#passwordHelperText').slideDown();
    });

    jQuery('#password').on('blur', function (e) {
      jQuery('#passwordHelperText').slideUp();
    });

    jQuery('#optin_search').change(function (e) {
      if (this.checked) {
        jQuery("#public-email-field").show();
        jQuery("#public-phone-field").show();
      } else {
        jQuery("#public-email-field").val('');
        jQuery("#public-email-field").hide();
        jQuery("#public-phone-field").val('');
        jQuery("#public-phone-field").hide();
      }
    });

    jQuery(document).ready(function (e) {
      jQuery('#optin_search').trigger('change');
    });

    jQuery('form.prescriber-form').on('submit', function (e) {
      e.preventDefault();
      jQuery(this).off();
      jQuery(this).find('button[type="submit"]').addClass('disabled');
      jQuery(this).submit();
    });

    jQuery("#public-phone").on("input", function (event) {
      var phoneRegex = /(\d{3})(\d{1,3})(\d{2,4})$/
      var intlCode = '+1';
      var phoneNumber = event.target.value.replace(intlCode, '').replace(/\D/g, '').substr(0, 10);

      event.target.value = phoneNumber ? intlCode + ' ' + phoneNumber.replace(phoneRegex, '($1) $2-$3') : '';
    });
  </script>
  <script src="/scripts/validation.js"></script>
  <script src="/scripts/register_validation_submit.js"></script>

