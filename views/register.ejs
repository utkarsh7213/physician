                  <input type="password" placeholder="Password" id="password" name="password" maxlength="255" required>

                  <div class="helper-field-text" id="passwordHelperText" style="display:none">Password must contain at
                    least 8 characters. At least 1 number, 1 lower case and 1 upper case letter.</div>
                </div>
                <div class="form-field">
                  <label for="confirm-password">Confirm Password:</label>
                  <input type="password" placeholder="Confirm Password" id="confirm-password"
                    name="password_confirmation" maxlength="255" required>
                </div>

                <div class="form-checkbox">
                  <input type="checkbox" id="privacy" value="agreed" name="agree" checked>
                  <label for="privacy">By registering you agree to our <a href="#" target="_self">Privacy Policy</a> and
                    our <a href="#" target="_self">Terms and
                      Conditions</a>.</label>
                </div>

                <!-- <div class="form-checkbox">
                  <input type="checkbox" id="baa" value="baa" name="baa">
                  <label for="baa">By registering you accept our <a href="#" target="_blank">BAA</a>.</label>
                </div> -->


              </div>
              <div class="form-submit text-center">
                <button type="submit" id="submit_btn">
                  <i class="fa fa-circle-o-notch fa-spin"></i>
                  Register
                </button>
              </div>
              <div class="form-account text-center">
                <h3>Already have an account?</h3>
                <a data-cy="login-button" href="/login" class="btn">Login</a>
              </div>
